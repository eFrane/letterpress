{
  "name": "efrane/letterpress",
  "description": "A configurable output formatting wrapper for PHP.",
  "authors": [
    {
      "name": "Stefan Graupner",
      "email": "stefan.graupner@gmail.com"
    }
  ],
  "license": "BSD",
  "require": {
    "php": ">=5.5.0",
    "illuminate/config": "5.1.*",
    "illuminate/support": "5.1.*",
    "symfony/finder": "2.7.*",
    "masterminds/html5": "^2.1",
    "jolicode/JoliTypo": "0.1.*",
    "erusev/parsedown": "1.6.*",
    "embed/embed": "1.8.*",
    "symfony/event-dispatcher": "^3.0"
  },
  "require-dev": {
    "raulfraile/ladybug": "*",
    "phpunit/phpunit": "^5.2"
  },
  "suggest": {
    "erusev/parsedown-extra": "Install this to use the markdown extra syntax."
  },
  "autoload": {
    "psr-4": {
      "EFrane\\Letterpress\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
